import UIKit

var numero = 0
for numero in 1...100
{
    if(numero % 5 == 0)
    {
        print("bingo\(numero)")
    }
    else if(numero % 2 == 0)
    {
        print("par \(numero)")
    }
    else if(numero > 30 && numero < 40)
    {
        print("viva swift \(numero)")
    }
    else
    {
        print("impar \(numero)")
    }
}
